@startuml

entity "지점" as 지점 {
    * 지점 코드
}

entity "담당자" as 담당자 {
    * 담당자 이름
    --
    담당자 코드
}

entity "고객" as 고객 {
    * 고객 코드
    --
    고객 이름
    주소
}

entity "주문" as 주문 {
    * 주문 번호
    --
    주문 날짜
}

entity "포장" as 포장 {
    * 포장 날짜
    --
    주문 ID (FK)
    담당자 ID (FK)
}

entity "주문 내역" as 주문내역 {
    * 제품
    --
    수량
    단가
    금액
}

entity "제품" as 제품 {
    * 제품
    --
    단가
}

지점 ||..o{ 주문
담당자 ||..o{ 주문
담당자 ||..o{ 포장
고객 ||..o{ 주문
주문 ||..o{ 포장
주문 ||..o{ 주문내역
제품 ||..o{ 주문내역

@enduml
